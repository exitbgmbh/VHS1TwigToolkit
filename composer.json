{
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "ccarnivore/twig-qrcode-extension",
                "version": "1.1",
                "source": {
                    "type" : "git",
                    "url" : "https://github.com/ccarnivore/twig-qrcode-extension.git",
                    "reference" : "master"
                }
            }
        }
    ],
    "require": {
        "ext-curl": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "twig/twig": "^3.0",
        "mpdf/mpdf": "^8.0",
        "symfony/http-foundation": "^5.1",
        "symfony/routing": "^5.1",
        "symfony/dependency-injection": "^5.1",
        "symfony/cache": "^5.1",
        "ccarnivore/twig-qrcode-extension": "1.1",
        "endroid/qr-code": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Priotas\\": "vendor/ccarnivore/twig-qrcode-extension/src/Priotas"
        }
    }
}
